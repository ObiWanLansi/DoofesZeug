<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <ApplicationIcon />
    <StartupObject />
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/ObiWanLansi/DoofesZeug</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ObiWanLansi/DoofesZeug</RepositoryUrl>
    <PackageTags>doof, zeug, objects, world</PackageTags>
    <Authors>ObiWanLansi</Authors>
    <Description>An collection of some usefuel models for rapid prototyping and / or creating test for stuff.</Description>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <Product>DoofesZeug</Product>
    <PackageId>DoofesZeug</PackageId>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RootNamespace>DoofesZeug</RootNamespace>
    <AssemblyName>DoofesZeug</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\colorbrewer.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\colorbrewer.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.Data.SQLite.Core" Version="1.0.115" />
    <PackageReference Include="YamlDotNet" Version="11.2.1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Src\Datatypes\Misc\" />
    <Folder Include="Src\Generated\" />
    <Folder Include="Src\Models\Science\Base\Length\" />
    <Folder Include="Src\Models\Science\Base\Weight\" />
    <Folder Include="Src\Models\Science\Chemistry\" />
    <Folder Include="Src\Models\Science\Mathematics\Geometry\" />
    <Folder Include="Src\Models\Science\Physics\" />
    <Folder Include="Src\Datasets\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\colorbrewer.json" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
